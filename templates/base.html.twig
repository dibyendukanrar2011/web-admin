<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
        <link href="{{asset('/admin/vendor/fontawesome-free/css/all.min.css')}}" rel="stylesheet" type="text/css">
        <link
            href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
            rel="stylesheet">
        <link href="{{asset('/admin/css/sb-admin-2.min.css')}}" rel="stylesheet">
        <link href="{{asset('/admin/vendor/datetimepicker-master/jquery.datetimepicker.min.css')}}" rel="stylesheet">
        <link href="{{asset('/admin/vendor/sweetalert2-11.7.3/sweetalert2.min.css')}}" rel="stylesheet">
        
        <script src="{{asset('/admin/vendor/jquery/jquery.min.js')}}"></script>
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <!-- Bootstrap core JavaScript-->
        <script src="{{asset('/admin/vendor/bootstrap/js/bootstrap.bundle.min.js')}}"></script>

        <!-- Core plugin JavaScript-->
        <script src="{{asset('/admin/vendor/jquery-easing/jquery.easing.min.js')}}"></script>

        <!-- Custom scripts for all pages-->
        <script src="{{asset('/admin/js/sb-admin-2.min.js')}}"></script>

        <!-- Page level plugins -->
        <script src="{{asset('/admin/vendor/chart.js/Chart.min.js')}}"></script>
        <script src="{{asset('/admin/vendor/datetimepicker-master/jquery.datetimepicker.full.js')}}"></script>
        
        <script src="{{asset('/admin/vendor/sweetalert2-11.7.3/sweetalert2.all.min.js')}}"></script>
        {% endblock %}
    </body>
</html>
